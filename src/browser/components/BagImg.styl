.bag-img
  > img
    height 98%
    float right
    //border 1px solid red

  .animateBackpackStyle
    transform scale(1.03)
    transition all .6s ease-in-out


@media (max-width: 767px)
  .bag-img
    > img
      display block
      margin 0 auto
      float none

@media (max-width: 480px)
  .bag-img
    > img
      height 95%
      width auto
      margin-top 2vw

