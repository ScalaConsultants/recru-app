.bag-img
  img
    height 90%
    float right
    //border 1px solid red

  .animateBackpackStyle
    transform scale(1.03)
    transition all .6s ease-in-out


@media (max-width: 767px)
  .bag-img
    img
      width 95%
      height auto
      display block
      margin 0 auto
      float none

@media (max-width: 480px)
  .bag-img
    img
      height 98%
      width auto
      margin-top 1vw

